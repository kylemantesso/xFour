#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/convex@1.29.3_@clerk+clerk-react@5.57.0_react-dom@19.2.0_react@19.2.0__react@19.2.0__react@19.2.0/node_modules/convex/bin/node_modules:/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/convex@1.29.3_@clerk+clerk-react@5.57.0_react-dom@19.2.0_react@19.2.0__react@19.2.0__react@19.2.0/node_modules/convex/node_modules:/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/convex@1.29.3_@clerk+clerk-react@5.57.0_react-dom@19.2.0_react@19.2.0__react@19.2.0__react@19.2.0/node_modules:/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/convex@1.29.3_@clerk+clerk-react@5.57.0_react-dom@19.2.0_react@19.2.0__react@19.2.0__react@19.2.0/node_modules/convex/bin/node_modules:/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/convex@1.29.3_@clerk+clerk-react@5.57.0_react-dom@19.2.0_react@19.2.0__react@19.2.0__react@19.2.0/node_modules/convex/node_modules:/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/convex@1.29.3_@clerk+clerk-react@5.57.0_react-dom@19.2.0_react@19.2.0__react@19.2.0__react@19.2.0/node_modules:/Users/kylemantesso/Documents/dev/x402-gateway/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../convex/bin/main.js" "$@"
else
  exec node  "$basedir/../convex/bin/main.js" "$@"
fi
