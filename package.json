{
  "name": "x402-gateway",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @x402/web dev",
    "build": "pnpm --filter @x402/web build",
    "build:sdk": "pnpm --filter @x402/sdk build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm --filter @x402/web exec tsc --noEmit && pnpm --filter @x402/sdk exec tsc --noEmit",
    "clean": "pnpm -r clean",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=18"
  }
}
